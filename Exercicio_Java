public class  main {
    public static void main(String[[] args]){

        Locale.setDefaut(Locale.US);


        int x, y;

        System.out.println("Digite dois numeros:");
        x = sc.nexTInt();
        y - sc.nexTInt();

        if (x > y){
            troca = x;
            x = y;
            y = troca;

        }
        soma = 0;
        for (int i = x+1; i < y; i++){
            if (i % 2 !=0){
                soma = soma + i;
            }
        }

        Systm.out.println("Soma dos IMPARES = " + soma);

        sc.close();
    }
}